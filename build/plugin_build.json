{
    "plugins": {
        "SysEventSource": {
            "path": "plugins/sysevent_source",
            "name": "sysevent_source"
        },
        "SysEventStore": {
            "path": "plugins/event_store",
            "name": "sys_event_store"
        },
        "Faultlogger": {
            "path": "plugins/faultlogger",
            "name": "faultlogger"
        },
        "EventLogger": {
            "path": "plugins/eventlogger",
            "name": "eventlogger"
        },
        "FreezeDetectorPlugin": {
            "path": "plugins/freeze_detector",
            "name": "freeze_detector"
        },
        "HiCollieCollector": {
            "path": "plugins/hicollie_collector",
            "name": "hicollie_collector"
        },
        "UsageEventReport": {
            "path": "plugins/usage_event_report",
            "name": "usage_event_report"
        },
        "BBoxDetectorPlugin": {
            "path": "plugins/reliability/bbox_detectors",
            "name": "bbox_detector"
        },
        "SysEventDispatcher": {
            "path": "plugins/sys_dispatcher",
            "name": "sys_dispatcher"
        }
    },
    "rules": [
        {
            "rule": [
                {
                    "rom": "1G",
                    "ram": "1G"
                }
            ],
            "info": {
                "loadorder": {
                    "SysEventSource": {
                        "loadtime": 0
                    },
                    "EventLogger": {
                        "loadtime": 0
                    },
                    "FreezeDetectorPlugin": {
                        "loadtime": 0
                    },
                    "HiCollieCollector": {
                        "loadtime": 0
                    },
                    "SysEventStore": {
                        "loadtime": 0
                    },
                    "Faultlogger": {
                        "loadtime": 0
                    },
                    "UsageEventReport": {
                        "loadtime": 0
                    },
                    "BBoxDetectorPlugin": {
                        "loadtime": 0
                    }, 
                    "SysEventDispatcher": {
                        "loadtime": 0
                    }
                },
                "pipelines": {
                    "SysEventPipeline": [
                        "SysEventDispatcher",
                        "SysEventStore",
                        "Faultlogger",
                        "EventLogger"
                    ],
                    "faultloggerPipeline": [
                        "Faultlogger",
                        "SysEventDispatcher",
                        "SysEventStore"
                    ],
                    "BBoxDetectorPipeline": [
                        "BBoxDetectorPlugin",
                        "SysEventDispatcher",
                        "SysEventStore"
                    ]
                },
                "pipelinegroups": {
                    "SysEventSource": [
                        "SysEventPipeline",
                        "faultloggerPipeline",
                        "BBoxDetectorPipeline"
                    ]
                },
                "threads": {
                    "singledthread": {
                        "SysEventSource": "sysevent_source",
                        "SysEventStore": "sysevent_store",
                        "EventLogger": "event_logger",
                        "FreezeDetectorPlugin": "freeze_detector",
                        "UsageEventReport": "usage_event_report",
                        "BBoxDetectorPlugin": "bbox_detector",
                        "SysEventDispatcher": "sys_dispatcher"
                    }
                }
            }
        }
    ]
}
